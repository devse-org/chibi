{
    "id": "chibi-x86_64",
    "type": "target",
    "props": {
        "toolchain": "clang",
        "arch": "x86_64",
        "sys": "chibi",
        "abi": "sysv",
        "encoding": "utf8",
        "freestanding": true,
        "host": false
    },
    "tools": {
        "cc": {
            "cmd": [
                "@latest",
                "clang"
            ],
            "args": [
                "-target",
                "x86_64-none-elf",
                "-ffreestanding",
                "-fno-stack-protector"
            ]
        },
        "cxx": {
            "cmd": [
                "@latest",
                "clang++"
            ],
            "args": [
                "-target",
                "x86_64-none-elf",
                "-ffreestanding",
                "-fno-stack-protector"
            ]
        },
        "ld": {
            "cmd": [
                "@latest",
                "ld.lld"
            ],
            "args": [
                "-m",
                "elf_x86_64",
                "-Tmeta/targets/link.ld",
                "-z",
                "max-page-size=0x1000"
            ]
        },
        "ar": {
            "cmd": [
                "@latest",
                "llvm-ar"
            ],
            "args": [
                "rcs"
            ]
        },
        "as": {
            "cmd": "nasm",
            "args": [
                "-f",
                "elf64"
            ]
        }
    }
}